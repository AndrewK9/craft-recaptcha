{
    "name": "codewithkyle/recaptcha",
    "description": "A simple craft plugin for validating Google's reCAPTCHA v3",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "recaptcha"
    ],
    "support": {
        "source": "https://github.com/codewithkyle/craft-recaptcha",
        "docs": "https://github.com/codewithkyle/craft-recaptcha",
        "issues": "https://github.com/codewithkyle/craft-recaptcha/issues",
        "support": "kylea@page.works"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Andrews",
            "homepage": "http://devkyle.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/codewithkyle/craft-recaptcha.git"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "codewithkyle\\recaptcha\\": "src/"
        }
    },
    "extra": {
        "name": "reCAPTCHA v3 for Craft",
        "handle": "recaptcha",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://github.com/codewithkyle/craft-recaptcha/blob/master/CHANGELOG.md",
        "components": {
            "recaptchaService": "codewithkyle\\recaptcha\\services\\RecaptchaService"
        },
        "class": "codewithkyle\\recaptcha\\Recaptcha"
    }
}
